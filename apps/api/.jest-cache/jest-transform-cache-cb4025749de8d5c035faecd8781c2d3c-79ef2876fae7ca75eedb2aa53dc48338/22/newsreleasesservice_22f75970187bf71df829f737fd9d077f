    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[25][1]++;
    }
    cov_rok3qerl7().s[54]++;
    if (
    /* istanbul ignore next */
    (cov_rok3qerl7().b[28][0]++, query.tags) &&
    /* istanbul ignore next */
    (cov_rok3qerl7().b[28][1]++, query.tags.length > 0)) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[27][0]++;
      cov_rok3qerl7().s[55]++;
      filter.tags = {
        $in: query.tags
      };
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[27][1]++;
    }
    cov_rok3qerl7().s[56]++;
    if (query.author) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[29][0]++;
      cov_rok3qerl7().s[57]++;
      filter['author.id'] = query.author;
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[29][1]++;
    }
    cov_rok3qerl7().s[58]++;
    if (
    /* istanbul ignore next */
    (cov_rok3qerl7().b[31][0]++, query.publishedAfter) ||
    /* istanbul ignore next */
    (cov_rok3qerl7().b[31][1]++, query.publishedBefore)) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[30][0]++;
      cov_rok3qerl7().s[59]++;
      filter.publishedAt = {};
      /* istanbul ignore next */
      cov_rok3qerl7().s[60]++;
      if (query.publishedAfter) {
        /* istanbul ignore next */
        cov_rok3qerl7().b[32][0]++;
        cov_rok3qerl7().s[61]++;
        filter.publishedAt.$gte = query.publishedAfter;
      } else
      /* istanbul ignore next */
      {
        cov_rok3qerl7().b[32][1]++;
      }
      cov_rok3qerl7().s[62]++;
      if (query.publishedBefore) {
        /* istanbul ignore next */
        cov_rok3qerl7().b[33][0]++;
        cov_rok3qerl7().s[63]++;
        filter.publishedAt.$lte = query.publishedBefore;
      } else
      /* istanbul ignore next */
      {
        cov_rok3qerl7().b[33][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[30][1]++;
    }
    cov_rok3qerl7().s[64]++;
    if (query.search) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[34][0]++;
      const searchRegex =
      /* istanbul ignore next */
      (cov_rok3qerl7().s[65]++, new RegExp(query.search, 'i'));
      /* istanbul ignore next */
      cov_rok3qerl7().s[66]++;
      filter.$or = [{
        'content.pt-BR.title': searchRegex
      }, {
        'content.en.title': searchRegex
      }, {
        'content.es.title': searchRegex
      }, {
        'content.pt-BR.content': searchRegex
      }, {
        'content.en.content': searchRegex
      }, {
        'content.es.content': searchRegex
      }];
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[34][1]++;
    }
    const sortOptions =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[67]++, {});
    /* istanbul ignore next */
    cov_rok3qerl7().s[68]++;
    if (
    /* istanbul ignore next */
    (cov_rok3qerl7().b[36][0]++, query.sortBy === 'title') &&
    /* istanbul ignore next */
    (cov_rok3qerl7().b[36][1]++, query.language)) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[35][0]++;
      cov_rok3qerl7().s[69]++;
      sortOptions[`content.${query.language}.title`] = query.sortOrder === 'asc' ?
      /* istanbul ignore next */
      (cov_rok3qerl7().b[37][0]++, 1) :
      /* istanbul ignore next */
      (cov_rok3qerl7().b[37][1]++, -1);
    } else {
      /* istanbul ignore next */
      cov_rok3qerl7().b[35][1]++;
      cov_rok3qerl7().s[70]++;
      sortOptions[query.sortBy] = query.sortOrder === 'asc' ?
      /* istanbul ignore next */
      (cov_rok3qerl7().b[38][0]++, 1) :
      /* istanbul ignore next */
      (cov_rok3qerl7().b[38][1]++, -1);
    }
    const total =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[71]++, await this.newsReleaseModel.countDocuments(filter));
    const pages =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[72]++, Math.ceil(total / query.limit));
    const skip =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[73]++, (query.page - 1) * query.limit);
    const items =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[74]++, await this.newsReleaseModel.find(filter).sort(sortOptions).skip(skip).limit(query.limit).exec());
    /* istanbul ignore next */
    cov_rok3qerl7().s[75]++;
    return {
      items,
      total,
      page: query.page,
      pages
    };
  }
