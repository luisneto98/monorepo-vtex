  async uploadImage(id, file, metadata) {
    /* istanbul ignore next */
    cov_rok3qerl7().f[14]++;
    const newsRelease =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[130]++, await this.findOne(id));
    const maxImages =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[131]++, 20);
    /* istanbul ignore next */
    cov_rok3qerl7().s[132]++;
    if (newsRelease.images.length >= maxImages) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[57][0]++;
      cov_rok3qerl7().s[133]++;
      throw new common_1.BadRequestException(`Maximum of ${maxImages} images allowed per article`);
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[57][1]++;
    }
    const {
      url,
      thumbnailUrl
    } =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[134]++, await this.imageProcessingService.uploadImage(file));
    const newImage =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[135]++, {
      _id: new this.newsReleaseModel().id,
      url,
      thumbnailUrl,
      caption:
      /* istanbul ignore next */
      (cov_rok3qerl7().b[58][0]++, metadata.caption) ||
      /* istanbul ignore next */
      (cov_rok3qerl7().b[58][1]++, {}),
      altText:
      /* istanbul ignore next */
      (cov_rok3qerl7().b[59][0]++, metadata.altText) ||
      /* istanbul ignore next */
      (cov_rok3qerl7().b[59][1]++, {}),
      order:
      /* istanbul ignore next */
      (cov_rok3qerl7().b[60][0]++, metadata.order) ||
      /* istanbul ignore next */
      (cov_rok3qerl7().b[60][1]++, newsRelease.images.length),
      uploadedAt: new Date()
    });
    const updated =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[136]++, await this.newsReleaseModel.findByIdAndUpdate(id, {
      $push: {
        images: newImage
      },
      $inc: {
        version: 1
      }
    }, {
      new: true
    }));
    /* istanbul ignore next */
    cov_rok3qerl7().s[137]++;
    return updated;
  }
  async removeImage(id, imageId) {
    /* istanbul ignore next */
    cov_rok3qerl7().f[15]++;
