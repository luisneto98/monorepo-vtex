  async reorderImages(id, imageIds) {
    /* istanbul ignore next */
    cov_rok3qerl7().f[17]++;
    const newsRelease =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[146]++, await this.findOne(id));
    const reorderedImages =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[147]++, imageIds.map((imageId, index) => {
      /* istanbul ignore next */
      cov_rok3qerl7().f[18]++;
      const image =
      /* istanbul ignore next */
      (cov_rok3qerl7().s[148]++, newsRelease.images.find(img => {
        /* istanbul ignore next */
        cov_rok3qerl7().f[19]++;
        cov_rok3qerl7().s[149]++;
        return img._id?.toString() === imageId;
