  async update(id, dto, user) {
    /* istanbul ignore next */
    cov_rok3qerl7().f[9]++;
    const newsRelease =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[84]++, await this.findOne(id));
    const updateData =
    /* istanbul ignore next */
    (cov_rok3qerl7().s[85]++, {
      $inc: {
        version: 1
      }
    });
    /* istanbul ignore next */
    cov_rok3qerl7().s[86]++;
    if (dto.content) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[42][0]++;
      cov_rok3qerl7().s[87]++;
      updateData.content = this.contentSanitizationService.sanitizeAllContent(dto.content);
      /* istanbul ignore next */
      cov_rok3qerl7().s[88]++;
      if (
      /* istanbul ignore next */
      (cov_rok3qerl7().b[44][0]++, dto.content['en']?.title) &&
      /* istanbul ignore next */
      (cov_rok3qerl7().b[44][1]++, dto.content['en'].title !== newsRelease.content['en'].title)) {
        /* istanbul ignore next */
        cov_rok3qerl7().b[43][0]++;
        cov_rok3qerl7().s[89]++;
        updateData.slug = await slug_generator_util_1.SlugGeneratorUtil.generateUniqueSlug(dto.content['en'].title, this.newsReleaseModel, id);
      } else
      /* istanbul ignore next */
      {
        cov_rok3qerl7().b[43][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[42][1]++;
    }
    cov_rok3qerl7().s[90]++;
    if (dto.status !== undefined) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[45][0]++;
      cov_rok3qerl7().s[91]++;
      updateData.status = dto.status;
      /* istanbul ignore next */
      cov_rok3qerl7().s[92]++;
      if (
      /* istanbul ignore next */
      (cov_rok3qerl7().b[47][0]++, dto.status === shared_1.NewsReleaseStatus.PUBLISHED) &&
      /* istanbul ignore next */
      (cov_rok3qerl7().b[47][1]++, !newsRelease.publishedAt)) {
        /* istanbul ignore next */
