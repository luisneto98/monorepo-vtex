      /* istanbul ignore next */
      {
        cov_rok3qerl7().b[46][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[45][1]++;
    }
    cov_rok3qerl7().s[94]++;
    if (dto.featured !== undefined) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[48][0]++;
      cov_rok3qerl7().s[95]++;
      updateData.featured = dto.featured;
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[48][1]++;
    }
    cov_rok3qerl7().s[96]++;
    if (dto.featuredImage !== undefined) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[49][0]++;
      cov_rok3qerl7().s[97]++;
      updateData.featuredImage = dto.featuredImage;
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[49][1]++;
    }
    cov_rok3qerl7().s[98]++;
    if (dto.categories !== undefined) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[50][0]++;
      cov_rok3qerl7().s[99]++;
      updateData.categories = dto.categories;
    } else
    /* istanbul ignore next */
    {
      cov_rok3qerl7().b[50][1]++;
    }
    cov_rok3qerl7().s[100]++;
    if (dto.tags !== undefined) {
      /* istanbul ignore next */
      cov_rok3qerl7().b[51][0]++;
      cov_rok3qerl7().s[101]++;
      updateData.tags = dto.tags;
    } else
    /* istanbul ignore next */
