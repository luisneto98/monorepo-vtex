    cov_110rxjezrb().s[17]++;
    return {
      'pt-BR': this.sanitizeLocalizedContent(content['pt-BR']),
      en: this.sanitizeLocalizedContent(content['en']),
      es: this.sanitizeLocalizedContent(content['es'])
    };
  }
};
