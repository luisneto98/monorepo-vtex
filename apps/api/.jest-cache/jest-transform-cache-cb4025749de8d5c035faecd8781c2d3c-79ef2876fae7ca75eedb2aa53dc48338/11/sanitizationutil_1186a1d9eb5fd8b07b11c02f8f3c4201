  static sanitizeTags(tags) {
    /* istanbul ignore next */
    cov_1lnp4fnw22().f[3]++;
    cov_1lnp4fnw22().s[21]++;
    if (
    /* istanbul ignore next */
    (cov_1lnp4fnw22().b[9][0]++, !tags) ||
    /* istanbul ignore next */
    (cov_1lnp4fnw22().b[9][1]++, !Array.isArray(tags))) {
      /* istanbul ignore next */
      cov_1lnp4fnw22().b[8][0]++;
      cov_1lnp4fnw22().s[22]++;
