  }
  static sanitizeFilePath(path) {
    /* istanbul ignore next */
    cov_1lnp4fnw22().f[5]++;
    cov_1lnp4fnw22().s[25]++;
    if (!path) {
      /* istanbul ignore next */
      cov_1lnp4fnw22().b[10][0]++;
      cov_1lnp4fnw22().s[26]++;
      return path;
    } else
    /* istanbul ignore next */
    {
      cov_1lnp4fnw22().b[10][1]++;
    }
    // Remove path traversal attempts
    cov_1lnp4fnw22().s[27]++;
    return path.replace(/\.\./g, '').replace(/^\/+/, '').replace(/\/+/g, '/').replace(/[^a-zA-Z0-9-_./]/g, '');
  }
